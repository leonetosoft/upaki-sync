    "shx": "^0.2.2",
    "socket.io-client": "^2.1.0",
    "sqlite3": "^4.0.2",
    "upaki-cli": "^1.3.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "awesome-typescript-loader": "^3.0.4-rc.2",
    "webpack": "^2.2.1",
    "@types/event-emitter": "^0.3.2",
    "@types/socket.io-client": "^1.4.32",
